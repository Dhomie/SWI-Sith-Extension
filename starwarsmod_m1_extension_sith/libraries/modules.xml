	<!-- Handle new Sith modules introduced by SWI 0.85 -->
	<add sel="/modules">
		<!-- Sith Habitation Modules -->
		<module id="hab_sith" group="hab_sith">
			<category tags="[habitation, module]" race="sith" faction="[sith]" />
		</module>
		
		<!-- Sith Production Modules -->
		<module id="prod_sith_computronicsubstrate" group="prod_sith_computronicsubstrate">
			<category ware="computronicsubstrate" tags="[production, module]" race="[sith]" faction="[sith]" />
			<compatibilities>
				<production ware="computronicsubstrate" chance="95" />
			</compatibilities>
		</module>
		
		<module id="prod_sith_metallicmicrolattice" group="prod_sith_metallicmicrolattice">
			<category ware="metallicmicrolattice" tags="[production, module]" race="[sith]" faction="[sith]" />
			<compatibilities>
				<production ware="metallicmicrolattice" chance="95" />
			</compatibilities>
		</module>
		
		<module id="prod_sith_proteinpaste" group="prod_sith_proteinpaste">
			<category ware="proteinpaste" tags="[production, module]" race="[sith]" faction="[sith]" />
			<compatibilities>
				<production ware="proteinpaste" chance="95" />
			</compatibilities>
		</module>
		
		<module id="prod_sith_siliconcarbide" group="prod_sith_siliconcarbide">
			<category ware="siliconcarbide" tags="[production, module]" race="[sith]" faction="[sith]" />
			<compatibilities>
				<production ware="siliconcarbide" chance="95" />
			</compatibilities>
		</module>
		
		<module id="prod_sith_mre" group="prod_sith_mre">
			<category ware="terranmre" tags="[production, module]" race="[sith]" faction="[sith]" />
			<compatibilities>
				<production ware="terranmre" chance="95" />
				<production ware="proteinpaste" chance="50" />
			</compatibilities>
		</module>
		
		<module id="prod_sith_shipsupplies" group="prod_sith_shipsupplies">
			<category ware="shipsupplies" tags="[production, module]" race="[sith]" faction="[sith]" />
			<compatibilities>
				<production ware="shipsupplies" chance="95" />
				<production ware="terranmre" chance="50" />
			</compatibilities>
		</module>
	</add>

